<!DOCTYPE html>
<html>
    <head>
        <title>Membres</title>
        <% include templates/header.ejs %>
    </head>
    <body>
        <header>
          <% include templates/navBar.ejs %>
        </header>
        <div class="container">
            <h1>Membres</h1>

            <table class="table table-condensed">
                <thead>
                <tr>
                    <th>Nom</th>
                    <th>Grade</th>
                    <th>Contact</th>
                </tr>
                </thead>
                <tbody>
                <tr>

                </tr>
                <% doc.forEach(function (doc) { %>
                <tr>
                    <td><a href="/profile?nom=<%=doc.nom%>"><%= doc.nom %> </a></td>
                    <td><%= doc.grade %></td>
                    <td><%= doc.email%></td>
                </tr>
                <% }) %>

                </tbody>
            </table>

            <% include templates/footer.ejs %>
        </div>




    </body>
</html>