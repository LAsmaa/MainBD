<!DOCTYPE html>
<html>
    <head>
        <title>Membres</title>
        <% include templates/header.ejs %>
    </head>
    <body>
        <header>
          <% include templates/navBar.ejs %>
        </header>
        <div class="containerMain">
            <div class="container">
                <table class="table table-condensed">
                    <thead>
                    <tr>
                        <th>Nom</th>
                        <th>Grade</th>
                        <th>Contact</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>

                    </tr>
                    <% doc.forEach(function (doc) { %>
                    <tr>
                        <td><a href="/profile?nom=<%=doc.nom%>"><%= doc.nom %> </a></td>
                        <td><%= doc.grade %></td>
                        <td><%= doc.email%></td>
                    </tr>
                    <% }) %>

                    </tbody>
                </table>

            </div>
        </div>


        <footer>
            <% include templates/footer.ejs %>
        </footer>
    </body>
</html>